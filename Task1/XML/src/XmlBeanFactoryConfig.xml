<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="motor1" class="com.aqil.manufacture.VehicleMotor">
    	<property name="name" value="PCX"></property>   
        <property name="wheel" value="2"></property>
        <property name="engineCapacity" value="150.0"></property>
        <property name="transmissionGear" value="0"></property>
        <property name="maxSpeed" value="160"></property>
    </bean>
    
    <bean id="car1" class="com.aqil.manufacture.VehicleCar">
    	<property name="name" value="Jazz"></property>   
        <property name="wheel" value="4"></property>
        <property name="engineCapacity" value="1199.0"></property>
        <property name="transmissionGear" value="5"></property>
        <property name="maxSpeed" value="220"></property>
    </bean>

</beans>